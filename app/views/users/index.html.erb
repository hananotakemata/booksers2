<p id="notice"></p>
<div class="container px-5 px-sm-0">
   <div class = "row">
  <div class = "col-md-3">
  <%= render 'users/list', user: @user, book: @book %>
  </div>

<div class="col-md-8 offset-md-1">
<h2>Users</h2>
<table class="table">
  <% @users.each do |user| %>
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
      <tr>
        <td> <%= image_tag user.get_profile_image(80,80) %></td>
        <td><%= user.name %></td>
        <td>
           <%= link_to user_path(user) do %>
            Show
           <% end %>
        </td>
      </tr>
  </tbody>
  <% end %>
</table>
 </div>
  </div>
   </div>